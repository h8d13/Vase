#!/bin/bash

# Check if sysbench is installed
if ! command -v sysbench >/dev/null 2>&1; then
    echo "Error: sysbench not found. Install it with: sudo pacman -S sysbench"
    exit 1
fi

# CPU benchmark
echo "Starting CPU benchmark..."
sysbench cpu --threads=$(nproc) --time=60 run 2>&1 

echo "CPU benchmark complete."